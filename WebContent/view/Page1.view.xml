<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="smax.empReg.view.Page1" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Page 1">
		<content>
	        <List items="{/ProductSet}">
	           <items>
	              <StandardListItem title="{ProductID}"
	              description="{Name}"
	              type="Active"
	              press="onItemPress"
	              />
	           </items>
	        </List>
		</content>
	</Page>
</core:View>